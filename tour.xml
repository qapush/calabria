		<preview url="panos/calabria_1.tiles/preview.jpg" />

		<image prealign="null">
			<cube url="panos/calabria_1.tiles/%s/l%l/%v/l%l_%s_%v_%h.jpg" multires="512,1024,2112,4224" />
